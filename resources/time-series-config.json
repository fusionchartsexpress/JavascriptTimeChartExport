[
  {
    "type": "timeseries",
    "renderAt": "chart-container",
    "width": "100%",
    "height": "100%",
    "creditLabel": false,
    "dataSource": {
      "data": {
        "schema": [
          {
            "name": "Time",
            "type": "date",
